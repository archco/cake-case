{
  "name": "cake-case",
  "version": "1.2.0",
  "description": "The simple utility methods that converts string case.",
  "main": "dist/cake-case.js",
  "module": "dist/module/cake-case.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "prebuild": "tsc",
    "build": "webpack --hide-modules",
    "watch": "webpack --hide-modules --watch"
  },
  "keywords": [
    "case",
    "convert"
  ],
  "author": "archcoster@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archco/cake-case.git"
  },
  "bugs": {
    "url": "https://github.com/archco/cake-case/issues"
  },
  "homepage": "https://github.com/archco/cake-case#readme",
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jest": "^24.0.19",
    "jest": "^24.9.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-notifier": "^1.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
